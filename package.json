{
  "name": "my-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages@1",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages@1 --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2023-10-16 --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@babel/preset-env": "^7.23.2",
    "@nextui-org/react": "^2.1.13",
    "@types/node": "20.11.2",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "10.4.16",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "framer-motion": "^10.16.4",
    "gray-matter": "^4.0.3",
    "next": "14.0.0",
    "next-themes": "^0.2.1",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "rss": "^1.2.2",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.7.2",
    "@types/rss": "^0.0.31",
    "eslint-plugin-next-on-pages": "^1.7.2",
    "vercel": "^32.5.0"
  }
